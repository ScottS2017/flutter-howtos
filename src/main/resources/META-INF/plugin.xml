<?xml version="1.0" encoding="UTF-8"?>
<idea-plugin>
    <id>com.stoll.scott.flutter-howtos</id>
    <name>Flutter HowTos</name>
    <vendor>Scott Stoll</vendor>
    <description><![CDATA[
For details of how to use the How Tos, please <a href="https://github.com/ScottS2017/flutter-howtos/blob/master/README.md">visit here</a>
]]></description>
    <idea-version since-build="141.0" />
    <depends>com.intellij.modules.java</depends>
    <extensions defaultExtensionNs="com.intellij">
        <defaultLiveTemplatesProvider implementation="FlutterTemplateProvider" />
        <liveTemplateContext implementation="FlutterContext" />
    </extensions>
</idea-plugin>
